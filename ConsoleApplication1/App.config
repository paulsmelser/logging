			<context type="Whatsnexx.Logging.Data.Context.LoggingContext, Whatsnexx.Logging">
				-->
		<!--<databaseInitializer type="Blogging.MyCustomBlogInitializer, MyAssembly" />-->
		<!--
			</context>
		</contexts>-->
	</entityFramework>
</configuration>